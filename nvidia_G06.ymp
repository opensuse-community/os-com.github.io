<metapackage xmlns:os="http://opensuse.org/Standards/One_Click_Install" xmlns="http://opensuse.org/Standards/One_Click_Install">
    <!--
        Maintained by the following authors:
        * Martin Schlander <mschlander@opensuse.org>
    -->
    <group distversion="openSUSE Tumbleweed">
        <name>NVIDIA Graphics Drivers</name>
        <summary>Proprietary nVidia Graphics Driver packages</summary>
        <description>
        This package provides the closed-source NVIDIA graphics driver for GeForce 600 series and newer GPUs.
        </description>
        <remainSubscribed/>
        <repositories>
            <repository recommended="true" format="auto">
                <name>nVidia Graphics Drivers</name>
                <summary>This will install the nVidia drivers from nvidia.com for you</summary>
                <description>This package provides the closed-source NVIDIA graphics driver for GeForce 600 series and newer GPUs.</description>
                <url>https://download.nvidia.com/opensuse/tumbleweed</url>
            </repository>
        </repositories>
        <software>
            <item recommended="true">
                <name>x11-video-nvidiaG06</name>
                <summary>NVIDIA GL Drivers</summary>
                <description>This package provides the closed-source NVIDIA graphics driver for GeForce 600 series and newer GPUs.</description>
            </item>
        </software>
     </group>
